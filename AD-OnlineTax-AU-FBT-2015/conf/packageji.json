{
  "name": "au-fbt-2015",
  "version": "0.1.0",
  "description": "Australia FBT 2015 module",
  "private": true,
  "main": "index.js",
  "engines": {
    "node": ">=0.10.25"
  },
  "scripts": {
    "selenium-install": "selenium-standalone install --version=2.43.1",
    "start": "node index.js",
    "test": "mocha --recursive spec/**/*.spec.js",
    "uitest": "./package-client build/test/form.tar.gz && tar zxf build/test/form.tar.gz -C build/test && mocha --slow 15000 --timeout 30000 --recursive spec/**/*.uispec.js",
    "cjtest": "mocha --slow 15000 --timeout 30000 --recursive spec/**/*.cjspec.js",
    "ctest": "mocha --slow 15000 --timeout 30000 --recursive spec/ui/loginpg006.cjspec.js",
    "rule-coverage": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -x 'spec/**/*' -x 'lib/!(calculation.js)' -x 'lib/validation/!(rules.js)' -- --ui tdd --reporter spec **/*.spec.js",
    "lint": "eslint *.js lib spec",
    "serve": "(cd browser && ../node_modules/.bin/beefy index.js -- --debug)",
    "upgrade-deps": "npm-check-updates --upgrade"
  },
  "dependencies": {
    "angular": "1.3.3",
    "archiver": "~0.6.1",
    "async": "^0.9.0",
    "big.js": "2.5.1",
    "bootstrap": "3.3.1",
    "bootstrap-sass": "3.3.1",
    "chai": "^2.1.1",
    "chai-as-promised": "^4.3.0",
    "chainit": "^2.1.1",
    "chromedriver": "^2.12.0",
    "css-parse": "^1.7.0",
    "css-value": "0.0.1",
    "deepmerge": "~0.2.7",
    "hogan": "1.0.2",
    "jquery": "2.1.1",
    "mocha": "^2.2.1",
    "node-uuid": "1.4.1",
    "pragma-singleton": "~1.0.3",
    "q": "^1.1.2",
    "request": "~2.34.0",
    "rgb2hex": "^0.1.0",
    "selenium-webdriver": "^2.44.0",
    "url": "^0.10.1",
    "wgxpath": "^0.23.0"
  },
  "devDependencies": {
    "beefy": "2.1.1",
    "browserify": "6.3.2",
    "browserify-ngannotate": "0.4.0",
    "csv": "0.4.1",
    "eslint": "0.10.1",
    "istanbul": "0.3.2",
    "node-sass": "2.4.8",
    "node-static": "0.7.6",
    "npm-check-updates": "1.5.0",
    "phantomjs": "1.9.13",
    "portfinder": "0.3.0",
    "selenium-standalone": "3.2.0",
    "uglifyify": "2.6.0",
    "webdriverio": "2.4.4",
    "webdriverjs-angular": "nicsnoek/webdriverjs-angular",
    "coveralls": "~2.8.0",
    "glob": "~3.2.7",
    "jshint": "~2.4.1",
    "nock": "~0.27.1",
    "should": "^3.3.2"
  },
  "browser": {
    "angular": "./node_modules/angular/angular.min.js",
    "jquery": "./node_modules/jquery/dist/jquery.min.js",
    "bootstrap": "./node_modules/bootstrap-sass/assets/javascripts/bootstrap.js"
  },
  "browserify": {
    "transform": [
      "browserify-ngannotate"
    ]
  },
  "keywords": [
    "tax",
    "ato",
    "fbt"
  ]
}
